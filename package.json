    "dependencies": {
        "moment": "^2.29.4",
        "typescript": "^4.7.4"
    },
    "devDependencies": {
        "@types/node": "^18.7.3",
        "cypress": "^10.4.0",
        "cypress-mailosaur": "^2.11.0",
        "cypress-xpath": "^2.0.1",
        "mailslurp-client": "^15.13.3",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^4.2.1",
        "mochawesome-report-generator": "^6.2.0"
    }
}
